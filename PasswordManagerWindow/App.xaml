             xmlns:local="clr-namespace:PasswordManagerWindow"
             StartupUri="MainWindow/MainWindow.xaml">
    <Application.Resources>
        
        <ResourceDictionary>
            
            <ResourceDictionary.MergedDictionaries>
                
                <ResourceDictionary Source="Styles/MainStyles.xaml" />
